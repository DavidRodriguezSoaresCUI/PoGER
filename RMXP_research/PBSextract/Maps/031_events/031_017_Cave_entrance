[EVENT]
xy=[55, 8]
pattern=3

[PAGE]
trigger=onPlayerTouch


Play SE="Door exit", volume=80, pitch=100
Change Screen Color Tone [255, 255, 255, 255], ms=300
Wait ms=400
s:pbCaveEntrance
Transfer Player map=34, x=23, y=17, direction="N", fading="NoFading"
Fadein ms=300
[end]


[PAGE]
trigger=onAutorun
s:isSSoff("A") == :ON

if s:get_character(0).onEvent?
  Set Move Route :player
    Play SE="Door exit", volume=80, pitch=100
    Step S
  
s:setTempSwitchOn("A")
[end]

