[EVENT]
xy=[2, 2]
trigger=onPlayerTouch

[PAGE]



Set Move Route :player
  Set property=through value=:ON
  Set property=always_on_top value=:ON
  Turn W
  Step NW
  Set property=through value=:OFF
  Set property=always_on_top value=:OFF
Transfer Player map=17, x=3, y=2, direction="E", fading="Fading"
[end]

