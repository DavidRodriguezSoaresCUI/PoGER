[EVENT]
xy=[9, 23]
pattern=3

[PAGE]
trigger=onPlayerTouch


Set Move Route :player
  Set property=through value=:ON
  Step N
  Set property=through value=:OFF
Play SE="Door exit", volume=80, pitch=100
Set property=transparent value=:ON
Fadeout color=Black, ms=300
Wait ms=400
Set property=transparent value=:OFF
Transfer Player map=60, x=5, y=7, direction="N", fading="NoFading"
Fadein ms=300
[end]


[PAGE]
trigger=onAutorun
s:isSSoff("A") == :ON

if s:get_character(0).onEvent?
  Set Move Route :player
    Play SE="Door exit", volume=80, pitch=100
    Step S
  
s:setTempSwitchOn("A")
[end]

